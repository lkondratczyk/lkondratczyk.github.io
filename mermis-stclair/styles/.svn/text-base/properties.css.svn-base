/************************************************************************************* * Stylesheet for properties.html * Matt Baker 2007 ************************************************************************************/ h1.properties{	margin-bottom:15px;	border-bottom:solid 1px #ccc;}h1.properties span{	font-size:0.8em;	font-style:italic;	font-variant:normal;	font-weight:normal;	color:#666;}img#propertyPic{	margin:5px 0;	height:285px;	border:none;}div#listing{	margin:0 auto 5px auto;	width:570px;	height:45px;	vertical-align:middle;}div#menu{	margin:0 auto;	padding-left:5px;	width:568px;	font-size:0;}* html div#menu { margin-left:10px; }div#menu br { clear:both; }div#menu img{	display:inline;	float:left;	margin:0 5px 5px 0;	padding:0;	width:66px;	height:48px;	border:none;	cursor:pointer;}p.propertyInfo{	display:none;	font-size:12.5px;	text-align:center;}p.propertyInfo span{	font-weight:bold;	color:#dd0000;}p.propertyInfo b { font-family:Verdana, Arial, Helvetica, sans-serif; color:#333 }